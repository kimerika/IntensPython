p = int(input("Введите значение p: "))
c = list(map(int, input("Введите целые числа c через пробел: ").split()))

count = 0  # счётчик чисел, меньших 20
for num in c:
    if num < 20:
        count += 1

if count == 5:
    print("Да, количество чисел, меньших 20, равно 5.")
else:
    print("Нет, количество чисел, меньших 20, не равно 5.")
