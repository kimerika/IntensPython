n = int(input("Введите количество грузов: "))  # вводим количество грузов
m = int(input("Введите грузоподъемность автомобиля: "))  # вводим грузоподъемность автомобиля
mass = 0  # инициализируем переменную для хранения суммарной массы грузов

for i in range(n):
    mass_i = int(input(f"Введите массу груза {i+1}: "))  # вводим массу каждого груза
    mass += mass_i  # добавляем массу груза к суммарной массе

if mass <= m:
    print("Общая масса всех грузов не превышает грузоподъемность автомобиля")
else:
    print("Общая масса всех грузов превышает грузоподъемность автомобиля")
