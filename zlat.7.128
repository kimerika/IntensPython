b = [b1, b2, b3, b4, b5, b6, b7, b8, b9, b10, b11] # список b1, ..., b11
has_7 = False # флаг наличия чисел, оканчивающихся на 7
b7_idx = None # порядковый номер первого числа, оканчивающегося на 7

for i in range(len(b)):
    if b[i] % 10 == 7: # проверяем, оканчивается ли число на 7
        b7_idx = i # сохраняем порядковый номер текущего числа
        has_7 = True # выставляем флаг наличия чисел, оканчивающихся на 7
        break

if has_7:
    print(f"Первый элемент, оканчивающийся на 7: {b[b7_idx]}, его индекс: {b7_idx}")
else:
    print("В последовательности отсутствуют числа, оканчивающиеся на 7")
