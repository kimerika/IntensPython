seq = [] # Создаем пустой список для последовательности чисел
num = None # Создаем переменную для хранения текущего числа
seq_index = 1 # Создаем переменную для хранения порядкового номера текущего числа

# Циклически запрашиваем у пользователя ввод чисел до тех пор, пока не будет введено число 100
while num != 100:
    num = int(input(f"Введите {seq_index}-e число: ")) # запрашиваем ввод числа
    seq.append(num) # добавляем число в список
    seq_index += 1 # увеличиваем порядковый номер текущего числа

if 666 in seq: # проверяем, содержится ли число 666 в последовательности
    print(f"Первый элемент, равный 666: {seq.index(666)+1}") # выводим порядковый номер первого элемента, равного 666
else:
    print("В последовательности отсутствует число 666") # выводим сообщение, если число 666 в последовательности не найдено
