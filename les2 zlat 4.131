a1 = float(input("Введите первое число первой тройки: "))
b1 = float(input("Введите второе число первой тройки: "))
c1 = float(input("Введите третье число первой тройки: "))

a2 = float(input("Введите первое число второй тройки: "))
b2 = float(input("Введите второе число второй тройки: "))
c2 = float(input("Введите третье число второй тройки: "))

average1 = (a1 + b1 + c1 - min(a1, b1, c1) - max(a1, b1, c1)) / 2
average2 = (a2 + b2 + c2 - min(a2, b2, c2) - max(a2, b2, c2)) / 2

average = (average1 + average2) / 2

print("Среднее арифметическое средних чисел каждой тройки: ", average)
