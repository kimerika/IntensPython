for i in range(10, 100):
    digits = [int(d) for d in str(i)]  # преобразование числа в список цифр
    sq_sum = sum(digit**2 for digit in digits)  # вычисление суммы квадратов цифр
    if sq_sum % 13 == 0:
        print(i)
        
for i in range(10, 100):
    digits = [int(d) for d in str(i)]  # преобразование числа в список цифр
    digit_sum = sum(digits)  # вычисление суммы цифр
    if i == digit_sum + digit_sum**2:
        print(i)
