num = int(input("Введите трехзначное число: "))
if num < 100 or num > 999:
    print("Ошибка ввода")
else:
    ones = num % 10
    tens = (num % 100) // 10
    hundreds = num // 100
    new_num = ones * 100 + hundreds * 10 + tens
    print("Число, полученное при перестановке второй и третьей цифр:", new_num)

