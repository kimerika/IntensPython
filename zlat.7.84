a = list(map(int, input("Введите 10 целых чисел через пробел: ").split()))

positive_count = 0  # счётчик положительных чисел
for num in a:
    if num > 0:
        positive_count += 1

if positive_count <= 5:
    print("Верно, что количество положительных чисел не превышает 5.")
else:
    print("Неверно, что количество положительных чисел не превышает 5.")
