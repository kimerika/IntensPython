import math 
 
# Вводим два числа 
num1 = float(input("Введите первое число: ")) 
num2 = float(input("Введите второе число: ")) 
 
# Вычисляем модули чисел 
mod1 = abs(num1) 
mod2 = abs(num2) 
 
# Вычисляем среднее арифметическое модулей 
avg_arithmetic = (mod1 + mod2) / 2 
 
# Вычисляем среднее геометрическое модулей 
avg_geometric = math.sqrt(mod1 * mod2) 
 
# Выводим результаты 
print("Среднее арифметическое модулей:", avg_arithmetic) 
print("Среднее геометрическое модулей:", avg_geometric)
