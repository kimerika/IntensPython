def has_real_roots(a, b, c):
    # Функция, которая проверяет наличие вещественных корней у квадратного уравнения ax^2 + bx + c
    d = b**2 - 4*a*c   # Вычисляем дискриминант

    if d >= 0:
        # Если дискриминант неотрицательный, то уравнение имеет вещественные корни
        return True
    else:
        return False

# Заданные уравнения
eq1 = (3, 4, 2)
eq2 = (4, 3, 2)
eq3 = (2, 4, 3)

eq_list = [eq1, eq2, eq3]   # Список уравнений
real_roots_count = 0        # Количество уравнений с вещественными корнями

for eq in eq_list:
    if has_real_roots(*eq):
        real_roots_count += 1

print("Количество уравнений с вещественными корнями:", real_roots_count)
