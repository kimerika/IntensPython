marks = [5, 4, 3, 4, 5, 5, 4, 4, 5, 3, 4, 5, 2, 4, 5, 5, 4, 3, 4, 3, 5, 4, 3, 4, 5, 2, 3, 4]  # оценки по информатике
has_twos = False  # флаг, сброшенный в False

for mark in marks:
    if mark == 2:  # если есть двойка
        has_twos = True  # устанавливаем флаг в True
        break  # прерываем цикл, т.к. остальные оценки не важны

if has_twos:
    print("Среди оценок есть двойки.")
else:
    print("Среди оценок нет двоек.")
