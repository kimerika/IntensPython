n = int(input("Введите натуральное число: "))  # принимаем число от пользователя

# находим его наименьший делитель
for i in range(2, n + 1):
    if n % i == 0:
        smallest_divisor = i  # запоминаем его и выходим из цикла
        break
    
# выводим результат
print("Наименьший делитель числа", n, "отличный от 1:", smallest_divisor)
