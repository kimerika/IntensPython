# Просим пользователя ввести числа и сохраняем их в список
numbers = []
while True:
    num = int(input('Введите число (0 для окончания ввода): '))
    if num == 0:
        break
    numbers.append(num)

# Сортируем список в порядке возрастания
numbers.sort()

# Выводим числа на экран, кроме нуля
for num in numbers:
    if num != 0:
        print(num)
