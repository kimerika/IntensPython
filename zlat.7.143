population = [10, 25, 6, 15, 30, 20, 12, 8, 18, 22, 28, 35, 7, 11, 17, 9] # список численности населения в миллионах жителей
area = [1.2, 3, 0.8, 2.5, 4, 2.8, 1.6, 0.9, 1.5, 2, 3.5, 5, 0.6, 1.1, 1.8, 0.7] # список площади в тысячах квадратных километров

min_density = population[0] / (area[0] * 1000) # задаем начальное значение для минимальной плотности населения

for i in range(len(population)): # проходим по всем элементам списков численности населения и площади из списка
    density = population[i] / (area[i] * 1000) # вычисляем плотность населения конкретного государства
    if density < min_density: # если плотность населения текущего государства меньше минимальной
        min_density = density # обновляем значение минимальной плотности населения

print('Минимальная плотность населения:', min_density) # выводим результат
