sequence = [10, -4, 12, 56, -4]  # заданная последовательность
sign_changes_count = 0  # счетчик изменений знака

for i in range(1, len(sequence)):
    if sequence[i] * sequence[i-1] < 0:  # если знаки соседних элементов разные
        sign_changes_count += 1  # увеличиваем счетчик

print("Количество изменений знака:", sign_changes_count)
