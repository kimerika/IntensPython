sequence = [1.0, 2.0, 3.0, 3.0, 4.0, 4.0, 5.0, 5.0, 5.0, 6.0, 7.0, 8.0, 9.0, 9.0, 9.0, 10.0, 11.0, 11.0, 11.0, 11.0, 1000.0]
count = 0

for i in range(len(sequence)-1):
    if sequence[i] == sequence[i+1]:
        count += 1
    if sequence[i+1] == 1000.0:
        break

print("Количество повторяющихся чисел:", count)
