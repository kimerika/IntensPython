n = int(input("Введите натуральное число: "))
m = int(input("Введите количество последних цифр для суммирования: "))

digits = []  # список для хранения последних цифр числа

while n > 0:
    digit = n % 10  # получаем очередную цифру
    digits.append(digit)  # добавляем цифру в список
    n //= 10

if m > len(digits):
    print("Ошибка! Количество цифр для суммирования больше, чем количество цифр в числе.")
else:
    sum_last_digits = sum(digits[:m])
    print("Cумма последних", m, "цифр числа", n, "равна", sum_last_digits)
