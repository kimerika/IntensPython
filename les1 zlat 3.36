x = 0
for i in range(1, 10):
    for j in range(0, 10):
        for k in range(0, 10):
            if i != j and j != k and i != k:
                # формируем число х
                x = i * 100 + k * 10 + j
                # проверяем условие
                if (x // 10) * 10 + j == 546:
                    print("Число x равно", x)
                    break
        else:
            continue
        break
    else:
        continue
    break
