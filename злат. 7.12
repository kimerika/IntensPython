a = []
value = int(input("Введите число: "))

while value != 0:
    a.append(value)
    value = int(input("Введите число: "))

product_list = [1]
temp_product = 1

for i in range(len(a)-1):
    temp_product *= a[i]
    product_list.append(temp_product)

for i in range(len(product_list)-1):
    print(product_list[i], end=" ")

print(0)
