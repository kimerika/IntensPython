a = float(input("Введите вещественное число а: "))  # ввод вещественного числа
n = int(input("Введите натуральное число n: "))  # ввод натурального числа

result = 1  # инициализация результата

for i in range(1, n+1):
    result *= a  # умножение текущего значения на a
    print(a, "^", i, "=", result)
