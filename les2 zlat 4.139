card = int(input("Введите номер карты (6-14): "))

if card == 6:
    print("Достоинство - шестерка")
elif card == 7:
    print("Достоинство - семерка")
elif card == 8:
    print("Достоинство - восьмерка")
elif card == 9:
    print("Достоинство - девятка")
elif card == 10:
    print("Достоинство - десятка")
elif card == 11:
    print("Достоинство - валет")
elif card == 12:
    print("Достоинство - дама")
elif card == 13:
    print("Достоинство - король")
elif card == 14:
    print("Достоинство - туз")
else:
    print("Некорректный номер карты")
