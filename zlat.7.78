fives = 0 # количество пятерок
fours = 0 # количество четверок
threes = 0 # количество троек
twos = 0 # количество двоек

num_of_students = int(input("Введите количество учеников: "))

for i in range(num_of_students):
  grade = int(input("Введите оценку ученика: ")) # ввод оценки ученика
  
  if grade == 5:
    fives += 1 # увеличиваем количество пятерок
  elif grade == 4:
    fours += 1 # увеличиваем количество четверок
  elif grade == 3:
    threes += 1 # увеличиваем количество троек
  elif grade == 2:
    twos += 1 # увеличиваем количество двоек

print("Количество пятерок:", fives)
print("Количество четверок:", fours)
print("Количество троек:", threes)
print("Количество двоек:", twos)
