temperatures = [20, 15, 18, 25, 20, 22, 24, 27, 28, 23, 20, 19] # список с температурами за месяц

max_temperature = temperatures[0] # задаем начальное значение для максимальной температуры

for temp in temperatures: # проходим по всем температурам из списка
    if temp > max_temperature: # если текущая температура больше максимальной
        max_temperature = temp # обновляем значение макисмальной температуры

print('Максимальная температура за месяц:', max_temperature) # выводим результат
