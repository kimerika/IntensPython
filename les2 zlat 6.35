num = int(input("Введите натуральное число: "))

# а) количество цифр 3 в нем
count_3 = 0
# б) сколько раз в нем встречается последняя цифра
count_last_digit = 0
last_digit = num % 10
# в) количество четных цифр в нем
count_even = 0
# г) сумму его цифр, больших пяти
sum_greater_than_5 = 0
# д) произведение его цифр, больших семи
prod_greater_than_7 = 1
# е) сколько раз в нем встречаются цифры 0 и 5 (всего)
count_0_5 = 0

while num > 0:
    digit = num % 10
    # а) количество цифр 3 в нем
    if digit == 3:
        count_3 += 1
    # б) сколько раз в нем встречается последняя цифра
    if digit == last_digit:
        count_last_digit += 1
    # в) количество четных цифр в нем
    if digit % 2 == 0:
        count_even += 1
    # г) сумму его цифр, больших пяти
    if digit > 5:
        sum_greater_than_5 += digit
    # д) произведение его цифр, больших семи
    if digit > 7:
        prod_greater_than_7 *= digit
    # е) сколько раз в нем встречаются цифры 0 и 5 (всего)
    if digit == 0 or digit == 5:
        count_0_5 += 1
    num //= 10

print("а) количество цифр 3 в нем:", count_3)
print("б) сколько раз в нем встречается последняя цифра:", count_last_digit)
print("в) количество четных цифр в нем:", count_even)
print("г) сумму его цифр, больших пяти:", sum_greater_than_5)
print("д) произведение его цифр, больших семи:", prod_greater_than_7)
print("е) сколько раз в нем встречаются цифры 0 и 5 (всего):", count_0_5)
