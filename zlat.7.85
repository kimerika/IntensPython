x = list(map(float, input("Введите 15 вещественных чисел через пробел: ").split()))

count = 0  # счётчик чисел, не больших 33.2 и кратных 4
for num in x:
    if num <= 33.2 and num % 4 == 0:
        count += 1

if count > 0:
    print("Да, существует хотя бы одно число, которое не больше 33.2 и кратно 4.")
else:
    print("Нет, нет чисел, не больших 33.2 и кратных 4.")
