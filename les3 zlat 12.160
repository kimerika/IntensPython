import re

# Исходный текст
text = 'В этом тексте есть 10 и 15, но и 20 тоже есть.'

# Находим все числа в тексте
numbers = re.findall(r'\d+', text)

# Преобразуем все числа из строкового формата в числовой формат
numbers = list(map(int, numbers))

# Определяем максимальное число
max_number = max(numbers)

# Выводим результат
print('Максимальное число:', max_number)
