string = input("Введите строку: ")
m = int(input("Введите номер символа, с которого начинается подстрока: "))
n = int(input("Введите номер символа, на котором заканчивается подстрока: "))
substring = string[m-1:n] # для учета индексации с нуля используем m-1 и n-1
if "666" in substring:
    print("Данная подстрока образует число 666")
else:
    print("Данная подстрока не образует число 666")
