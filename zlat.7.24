p = int(input("Введите натуральное число p: "))
n = int(input("Введите количество вещественных чисел: "))

s = float(input("Введите число s: "))
product = 1.0

for i in range(n):
  d = float(input(f"Введите вещественное число d{i+1}: "))
  product *= d
  
if product > s:
  print("Произведение вещественных чисел больше s.")
else:
  print("Произведение вещественных чисел меньше или равно s.")
