a = int(input("Введите номер вертикали первой клетки: "))
b = int(input("Введите номер горизонтали первой клетки: "))
c = int(input("Введите номер вертикали второй клетки: "))
d = int(input("Введите номер горизонтали второй клетки: "))
e = int(input("Введите номер вертикали третьей клетки: "))
f = int(input("Введите номер горизонтали третьей клетки: "))

# проверка для ладьи
if a == e or b == f:
    print("Ладья не может попасть на поле ({}, {})".format(e, f))
else:
    print("Ладья может попасть на поле ({}, {})".format(e, f))

# проверка для ферзя (комбинация ладьи и слона)
if a == e or b == f or abs(a - e) == abs(b - f):
    print("Ферзь не может попасть на поле ({}, {})".format(e, f))
else:
    print("Ферзь может попасть на поле ({}, {})".format(e, f))

# проверка для коня
if (abs(a - e) == 2 and abs(b - f) == 1) or (abs(a - e) == 1 and abs(b - f) == 2):
    print("Конь не может попасть на поле ({}, {})".format(e, f))
else:
    print("Конь может попасть на поле ({}, {})".format(e, f))

# проверка для слона
if abs(a - e) != abs(b - f):
    print("Слон не может попасть на поле ({}, {})".format(e, f))
else:
    # проверяем, не пересекает ли он путь другой фигуры
    delta_a = 1 if a < e else -1
    delta_b = 1 if b < f else -1
    i = a + delta_a
    j = b + delta_b
    while i != e and j != f:
        if i == c and j == d:
            print("Слон не может попасть на поле ({}, {}), так как на его пути находится черная фигура".format(e, f))
            break
        i += delta_a
        j += delta_b
    else:
        print("Слон может попасть на поле ({}, {})".format(e, f))

# проверка для короля
if abs(a - e) <= 1 and abs(b - f) <= 1:
    # проверяем, не находится ли он под ударом другой фигуры
    if abs(a - c) <= 1 and abs(b - d) <= 1:
        print("Король не может попасть на поле ({}, {}), так как находится под ударом черной фигуры".format(e, f))
    else:
        print("Король может попасть на поле ({}, {})".format(e, f))
else:
    print("Король не может попасть на поле ({}, {})".format(e, f))
