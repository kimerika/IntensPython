number = int(input("Введите натуральное число: "))

# Инициализируем переменные для максимальных и минимальных цифр
max1 = max2 = 0
min1 = min2 = 9 

# Проходимся по каждой цифре в числе
while number > 0:
    digit = number % 10
    # Если текущая цифра больше двух максимальных цифр, то обновляем значения
    if digit > max1:
        max2 = max1
        max1 = digit
    # Если текущая цифра больше второй максимальной цифры, но меньше первой, то обновляем значения
    elif digit > max2:
        max2 = digit
    # Если текущая цифра меньше двух минимальных цифр, то обновляем значения
    if digit < min1:
        min2 = min1
        min1 = digit
    # Если текущая цифра меньше второй минимальной цифры, но больше первой, то обновляем значения
    elif digit < min2:
        min2 = digit
    number //= 10

# Вывод максимальных и минимальных цифр
print(f"Две максимальные цифры: {max1}, {max2}")
print(f"Две минимальные цифры: {min1}, {min2}")
