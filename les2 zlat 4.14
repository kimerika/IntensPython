R1 = float(input("Введите сопротивление первого участка цепи: "))
U1 = float(input("Введите напряжение на первом участке цепи: "))
I1 = U1 / R1
R2 = float(input("Введите сопротивление второго участка цепи: "))
U2 = float(input("Введите напряжение на втором участке цепи: "))
I2 = U2 / R2
if I1 < I2:
    print("Меньший ток протекает по первому участку цепи")
else:
    print("Меньший ток протекает по второму участку цепи")
