# считываем четырехзначное число с консоли
number = input("Введите четырехзначное число: ")

# проверяем, входит ли цифра 4
if '4' in number:
    print("Входит цифра 4")
else:
    print("Не входит цифра 4")

# проверяем, входит ли цифра B
if 'B' in number:
    print("Входит цифра B")
else:
    print("Не входит цифра B")
