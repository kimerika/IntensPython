word = "программирование_"
k = 5  # индекс буквы, после которой нужно вставить символ
symbol = "m"  # буква, которую нужно вставить

# смещение всех символов начиная с (k+1)-го вправо на одну позицию
word = word[:k+1] + " " + word[k+1:]
for i in range(len(word)-2, k, -1):
    word = word[:i] + word[i+1] + word[i] + word[i+2:]

# присваивание (k+1)-му символу строки значения символа
word = word[:k+1] + symbol + word[k+2:]

print(word)  # выведет: программимрование_
