numbers = [6, 3, 8, 5, 6, 2, 9, 1, 3, 7, 7, 4, 5, 8, 2]  # последовательность чисел
pair_found = False  # флаг, сброшенный в False

for i in range(1, len(numbers)):
    if numbers[i] == numbers[i-1]:  # если числа соседние и равны
        pair_found = True  # устанавливаем флаг в True
        print("Первая пара одинаковых соседних чисел имеет порядковые номера:", i, i+1)  # выводим порядковые номера
        break  # прерываем цикл, т.к. нам важна только первая пара

if not pair_found:
    print("В последовательности нет пары одинаковых соседних чисел.")
