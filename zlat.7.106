numbers = [27.5, 18.9, 22.3, 19.8, 25.1, 20.3, 23.2, 21.7, 28.6, 26.9, 20.8, 21.9, 24.5, 20.6, 22.9, 23.8]

total = 0  # счетчик для хранения суммы чисел, которые больше 20
count = 0  # счетчик для хранения количества чисел, которые больше 20

for number in numbers:
    if number > 20:
        total += number
        count += 1
        
average = total / count

print("Среднее арифметическое чисел, больших 20, равно", average)
