p = int(input("Введите натуральное число p: "))  # вводим число p
b = [int(i) for i in input("Введите 10 целых чисел через пробел: ").split()]  # вводим 10 чисел b
sum_b = sum(b)  # считаем сумму чисел b

if sum_b < p:
    print("Сумма чисел b меньше p")
else:
    print("Сумма чисел b не меньше p")
