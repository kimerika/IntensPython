def is_palindrome(n):
    """Функция для определения, является ли число палиндромом"""
    # Переводим число в строку и сравниваем его с обратной строкой
    return str(n) == str(n)[::-1]

def has_palindrome(a, b):
    """Функция для проверки, является ли одно из двух чисел палиндромом"""
    return is_palindrome(a) or is_palindrome(b)

# Пример использования функции
print(has_palindrome(12345, 678876))  # True
print(has_palindrome(123, 456))  # False
print(has_palindrome(12321, 78987))  # True
