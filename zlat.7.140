circle_areas = [10, 20, 12, 25, 15] # список площадей кругов

min_radius = (circle_areas[0] / 3.14)  0.5 # задаем начальное значение для минимального радиуса

for area in circle_areas: # проходим по всем площадям кругов из списка
    radius = (area / 3.14)  0.5 # вычисляем радиус текущего круга
    if radius < min_radius: # если радиус текущего круга меньше минимального
        min_radius = radius # обновляем значение минимального радиуса

print('Радиус самого маленького круга:', min_radius) # выводим результат
