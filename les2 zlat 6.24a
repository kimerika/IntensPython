n = int(input("Введите натуральное число: "))  # принимаем число от пользователя
digits = []  # список для хранения цифр числа

# заполняем список цифрами числа
while n > 0:
    digits.append(n % 10)
    n //= 10
    
# вычисляем знакочередующуюся сумму цифр
sum = 0
for i in range(len(digits)):
    sum += (-1) ** i * digits[i]
    
# выводим результат
print("Знакочередующаяся сумма цифр числа:", sum)
