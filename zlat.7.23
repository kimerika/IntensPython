a = [int(i) for i in input("Введите 8 целых чисел через пробел: ").split()]  # вводим 8 чисел a
product_a = 1  # инициализируем переменную для хранения произведения чисел

for i in a:
    product_a *= i  # перемножаем все числа из списка

if product_a < 10000:
    print("Произведение чисел a меньше 10 000")
else:
    print("Произведение чисел a не меньше 10 000")
