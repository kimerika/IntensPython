б) u = [u2, u3, u4, u5, u6, u7, u8, u9, u10, u11, u12, u13, u14, u15] # список у2, ..., у15
u.sort() # сортируем список по возрастанию
p = 5.5  # задаем значение числа п
lower_idx = None # переменная для хранения порядкового номера первого элемента
upper_idx = None # переменная для хранения порядкового номера второго элемента

for i in range(len(u)):
    if u[i] > p: # находим первый элемент больше числа п
        upper_idx = i # сохраняем порядковый номер
        break

for i in range(len(u)-1, -1, -1):
    if u[i] < p: # находим первый элемент меньше числа п
        lower_idx = i # сохраняем порядковый номер
        break

# выводим результат
print(f"First element: {u[lower_idx]}, index: {lower_idx}")
print(f"Second element: {u[upper_idx]}, index: {upper_idx}")
