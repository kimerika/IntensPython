n = int(input("Введите номер квартиры: "))
if n < 1 or n > 216:
    print("Такой квартиры нет в доме")
else:
    entrance = (n - 1) // 54 + 1
    floor = ((n - 1) % 54) // 6 + 1
    number = (n - 1) % 6 + 1
    print(f"Квартира находится в {entrance} подъезде")
    print(f"Квартира находится на {floor} этаже")
    print(f"По счету на этаже она является {number}-й")

