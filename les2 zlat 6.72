f = int(input("Введите первый член прогрессии: "))
s = int(input("Введите шаг прогрессии: "))
p = int(input("Введите число: "))

if s == 0:
    if f == p:
        print(p, "является членом прогрессии")
    else:
        print(p, "не является членом прогрессии")
else:
    n = (p - f) / s
    if n == int(n):
        print(p, "является членом прогрессии")
    else:
        print(p, "не является членом прогрессии")
