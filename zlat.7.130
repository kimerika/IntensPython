numbers = [...] # задаем список из 20 чисел
duplicates = {} # создаем пустой словарь для хранения повторяющихся чисел

for num in numbers:
    if num in duplicates:
        duplicates[num] += 1 # если число уже встречалось, увеличиваем на 1 значение в словаре
    else:
        duplicates[num] = 1 # если число встречается впервые, добавляем его в словарь

for num, count in duplicates.items():
    if count > 1: # если количество повторений числа больше 1, выводим его значение и количество повторений
        print(f"Число {num} повторяется {count} раз(а)")
