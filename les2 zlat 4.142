months = ["январь", "февраль", "март", "апрель", "май", "июнь",
          "июль", "август", "сентябрь", "октябрь", "ноябрь", "декабрь"]

p = int(input("Введите количество прошедших месяцев: "))
d = int(input("Введите количество прошедших дней: "))

# Считаем количество дней, прошедших с начала 2010 года
days = p * 30 + d + 2

# Определяем номер месяца
month_number = days // 30

# Выводим название месяца
print(months[month_number])
