k = int(input("Введите номер дня в году: "))
d = int(input("Введите номер дня недели, на которую приходится 1 января: "))

# Определяем, каким днем недели является k-й день года
day_of_week = (k + d - 2) % 7 + 1

# Выводим результат
if day_of_week == 1:
    print("Понедельник")
elif day_of_week == 2:
    print("Вторник")
elif day_of_week == 3:
    print("Среда")
elif day_of_week == 4:
    print("Четверг")
elif day_of_week == 5:
    print("Пятница")
elif day_of_week == 6:
    print("Суббота")
elif day_of_week == 7:
    print("Воскресенье")
