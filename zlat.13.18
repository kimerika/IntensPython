students = [
    {
        "Фамилия": "Иванов",
        "Имя": "Иван",
        "Адрес": "г. Москва, ул. Ленина, д. 10",
        "Школа": 1,
        "Класс": 9
    },
    {
        "Фамилия": "Петров",
        "Имя": "Петр",
        "Адрес": "г. Москва, ул. Красного Маяка, д. 5",
        "Школа": 2,
        "Класс": 11
    },
    {
        "Фамилия": "Сидоров",
        "Имя": "Алексей",
        "Адрес": "г. Москва, ул. Пушкинская, д. 20",
        "Школа": 1,
        "Класс": 10
    },
    # Добавьте сюда остальных учеников
]

# Создаём пустой список для учеников в старших классах
senior_students = []

# Задаем номер интересующей нас школы и проходим циклом по всем ученикам,
# проверяя, учатся ли они в данной школе и находятся ли в старших классах
school_number = 1
for student in students:
    if student["Школа"] == school_number and student["Класс"] >= 10:
        senior_students.append({
            "Фамилия": student["Фамилия"],
            "Имя": student["Имя"],
            "Адрес": student["Адрес"]
        })

# Выводим данные найденных учеников в старших классах
print("Данные учеников школы №{} в старших классах:".format(school_number))
for student in senior_students:
    print("Фамилия: {}, Имя: {}, Адрес: {}".format(student["Фамилия"], student["Имя"], student["Адрес"]))
