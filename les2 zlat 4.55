# считываем четырехзначное число с консоли
number = input("Введите четырехзначное число: ")

# проверяем, входят ли цифры 2 или 7
if '2' in number or '7' in number:
    print("Входят цифры 2 или 7")
else:
    print("Не входят цифры 2 или 7")

# проверяем, входят ли цифры 3, 6 или 9
if set('369') & set(number):
    print("Входят цифры 3, 6 или 9")
else:
    print("Не входят цифры 3, 6 или 9")
