sentence = input("Введите предложение: ")
m_letters = ""
n_letters = ""

for letter in sentence:
    if letter == "м" or letter == "М":
        m_letters += letter
    elif letter == "н" or letter == "Н":
        n_letters += letter

print("Буквы 'м' в предложении: " + m_letters)
print("Буквы 'н' в предложении: " + n_letters)

