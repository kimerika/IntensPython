a) # Получаем n и создаем список из n целых чисел
n = int(input("Введите n: "))
numbers = []
for i in range(n):
    number = int(input(f"Введите число m{i+1}: "))
    numbers.append(number)

# Инициализируем переменную для суммы чисел меньше 25.5
sum_lt_25_5 = 0

# Считаем сумму чисел меньше 25.5 и проверяем условие
for num in numbers:
    if num < 25.5:
        sum_lt_25_5 += num
if sum_lt_25_5 <= 50:
    print("Сумма чисел меньше 25.5 не превышает 50.")
else:
    print("Сумма чисел меньше 25.5 превышает 50.")
b) # Получаем n и создаем список из n целых чисел
n = int(input("Введите n: "))
numbers = []
for i in range(n):
    number = int(input(f"Введите число m{i+1}: "))
    numbers.append(number)

# Инициализируем переменную для суммы чисел не превышающих 20
sum_le_20 = 0

# Считаем сумму чисел не превышающих 20 и проверяем условие
for num in numbers:
    if num <= 20:
        sum_le_20 += num
if sum_le_20 % 3 == 0:
    print("Сумма чисел, не превышающих 20, кратна трем.")
else:
    print("Сумма чисел, не превышающих 20, не кратна трем.")
