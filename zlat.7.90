marks = [4, 5, 3, 4, 5, 5, 4, 4, 5, 5, 5, 4]  # оценки ученика
has_threes = False  # флаг, сброшенный в False

for mark in marks:
    if mark == 3:  # если есть тройка
        has_threes = True  # устанавливаем флаг в True
        break  # прерываем цикл, т.к. остальные оценки не важны

if has_threes:
    print("Среди оценок есть тройки.")
else:
    print("Среди оценок нет троек.")
