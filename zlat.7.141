square_areas = [25, 16, 20, 32, 18, 28, 24, 30, 36, 40] # список площадей квадратов

max_diagonal = (2 * square_areas[0])  0.5 # задаем начальное значение для максимальной длины диагонали

for area in square_areas: # проходим по всем площадям квадратов из списка
    diagonal = (2 * area)  0.5 # вычисляем длину диагонали текущего квадрата
    if diagonal > max_diagonal: # если длина диагонали текущего квадрата больше максимальной
        max_diagonal = diagonal # обновляем значение максимальной длины диагонали

print('Длина диагонали самого большого квадрата:', max_diagonal) # выводим результат
