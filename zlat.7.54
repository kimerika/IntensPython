# Получаем п и создаем список из n вещественных чисел
p = int(input("Введите p: "))
n = int(input("Введите количество вещественных чисел: "))
numbers = []
for i in range(n):
    number = float(input(f"Введите число d{i+1}: "))
    numbers.append(number)

# Инициализируем переменную для суммы чисел больше 20.5
sum_gt_20_5 = 0

# Считаем сумму чисел больше 20.5 и проверяем условие
for num in numbers:
    if num > 20.5:
        sum_gt_20_5 += num
if sum_gt_20_5 < p:
    print("Сумма чисел больше 20.5 меньше p.")
else:
    print("Сумма чисел больше 20.5 не меньше p.")
