num = int(input("Введите трехзначное число: "))
if num < 100 or num > 999:
    print("Ошибка ввода")
else:
    ones = num % 10
    tens = (num % 100) // 10
    hundreds = num // 100
    new_num = tens * 100 + ones * 10 + hundreds
    print("Число, полученное при перестановке первой и второй цифр:", new_num)

