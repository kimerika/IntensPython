text = "This is an example of text with no five consecutive characters."

for i in range(len(text) - 4):
    if text[i] == text[i+1] == text[i+2] == text[i+3] == text[i+4]:
        print("The text contains five consecutive characters: ", text[i])
        break
else:
    print("The text does not contain five consecutive characters.")
