a = float(input("Введите ширину конверта: "))
b = float(input("Введите высоту конверта: "))
e = float(input("Введите ширину открытки: "))
d = float(input("Введите высоту открытки: "))

if a >= e + 2 and b >= d + 2:
    print("Открытку можно поместить в конверт")
else:
    print("Открытку нельзя поместить в конверт")
