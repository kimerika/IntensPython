def calculate_v(n):
    # Создаем и заполняем список значениями v1, v2, v3
    v = [0, 0, 1.5]
    # Вычисляем последующие значения v(i) в цикле начиная с индекса 3
    for i in range(3, n+1):
        v.append(((i-1)/(i**2+1))*v[i-1] - v[i-2] + v[i-3])
    # Возвращаем значение v(n)
    return v[n]

# Пример использования функции
n = 8
result = calculate_v(n)
print(f"v({n}) = {result}")
