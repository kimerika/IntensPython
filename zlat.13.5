    "Кинжал": 3305,
    "Эшторильоса": 4070,
    "Пайку": 4807,
    "Дхаулагири": 8167
}

# создаем список вершин, высота которых превышает 3000 метров
high_mountains = []
for mountain, elevation in mountains.items():
    if elevation > 3000:
        high_mountains.append(mountain)

# выводим список горных вершин с высотой более 3000 м.
print("Горные вершины, высота которых превышает 3000 м:")
for mountain in high_mountains:
    print(mountain)
