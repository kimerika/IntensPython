# задаем словарь с информацией о высоте горных вершин
mountains = {
    "Эльбрус": 5642,
    "Казбек": 5033,
    "Домбай-Улан-Тау": 4046,
    "Эверест": 8848,
    "Монблан": 4808,
    "Маттерхорн": 4477,
    "Макалу": 8485,
    "Аконкагуа": 6962,
    "Килиманджаро": 5895,
    "Кишима": 2956,
